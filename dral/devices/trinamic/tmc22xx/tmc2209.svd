<?xml version="1.0" encoding="utf-8" standalone="no"?>
<device schemaVersion="1.1"
xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
xs:noNamespaceSchemaLocation="CMSIS-SVD_Schema_1_1.xsd">
  <name>TMC2209</name>
  <version>1.03</version>
  <description>Step/Dir Drivers for Two-Phase Bipolar Stepper Motors up to 2.8A peak – StealthChop™ for Quiet Movement – UART Interface Option – Sensorless Stall Detection StallGuard4</description>
  <peripherals>
    <peripheral>
      <name>General</name>
      <description>General Registers</description>
      <baseAddress>0x00</baseAddress>
      <registers>
        <register>
          <name>GCONF</name>
          <displayName>GCONF</displayName>
          <description>Global configuration flags</description>
          <addressOffset>0x00</addressOffset>
          <size>10</size>
          <resetValue>0</resetValue>
          <fields>
            <field>
              <name>I_scale_analog</name>
              <description></description>
              <bitOffset>0</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>internal_Rsense</name>
              <description></description>
              <bitOffset>1</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>en_SpreadCycle</name>
              <description></description>
              <bitOffset>2</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>shaft</name>
              <description></description>
              <bitOffset>3</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>index_otpw</name>
              <description></description>
              <bitOffset>4</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>index_step</name>
              <description></description>
              <bitOffset>5</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>pdn_disable</name>
              <description></description>
              <bitOffset>6</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>mstep_reg_select</name>
              <description></description>
              <bitOffset>7</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>multistep_filt</name>
              <description></description>
              <bitOffset>8</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>test_mode</name>
              <description></description>
              <bitOffset>9</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        <register>
          <name>GSTAT</name>
          <displayName>GSTAT</displayName>
          <description>Global status flags</description>
          <addressOffset>0x01</addressOffset>
          <size>3</size>
          <resetValue>0</resetValue>
          <fields>
            <field>
              <name>reset</name>
              <description></description>
              <bitOffset>0</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>drv_err</name>
              <description></description>
              <bitOffset>1</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>uv_cp</name>
              <description></description>
              <bitOffset>2</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <register>
          <name>IFCNT</name>
          <displayName>IFCNT</displayName>
          <description>Interface transmission counte</description>
          <addressOffset>0x02</addressOffset>
          <size>8</size>
          <resetValue>0</resetValue>
          <fields>
            <field>
              <name>counter</name>
              <description></description>
              <bitOffset>0</bitOffset>
              <bitWidth>8</bitWidth>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <register>
          <name>SLAVECONF</name>
          <displayName>SLAVECONF</displayName>
          <description>SENDDELAY for read access (time until reply is sent)</description>
          <addressOffset>0x03</addressOffset>
          <size>4</size>
          <resetValue>0</resetValue>
        </register>

        <register>
          <name>OTP_PROG</name>
          <displayName>OTP_PROG</displayName>
          <description>OTP_PROGRAM – OTP programming</description>
          <addressOffset>0x04</addressOffset>
          <size>16</size>
          <resetValue>0</resetValue>
        </register>

        <register>
          <name>OTP_READ</name>
          <displayName>OTP_READ</displayName>
          <description>OTP_READ (Access to OTP memory result and update)</description>
          <addressOffset>0x05</addressOffset>
          <size>24</size>
          <resetValue>0</resetValue>
        </register>

        <register>
          <name>IOIN</name>
          <displayName>IOIN</displayName>
          <description>INPUT (Reads the state of all input pins available)</description>
          <addressOffset>0x06</addressOffset>
          <size>18</size>
          <resetValue>0</resetValue>
        </register>

        <register>
          <name>FACTORY_CONF</name>
          <displayName>FACTORY_CONF</displayName>
          <description>Factory config</description>
          <addressOffset>0x07</addressOffset>
          <size>7</size>
          <resetValue>0</resetValue>
        </register>
      </registers>
    </peripheral>

    <peripheral>
      <name>Velocity</name>
      <description>Velocity Dependent Control</description>
      <baseAddress>0x10</baseAddress>
      <registers>
      </registers>
    </peripheral>

    <peripheral>
      <name>StallGuard</name>
      <description>StallGuard Control</description>
      <baseAddress>0x14</baseAddress>
      <registers>
      </registers>
    </peripheral>

    <peripheral>
      <name>Sequencer</name>
      <description>Sequencer Registers</description>
      <baseAddress>0x60</baseAddress>
      <registers>
      </registers>
    </peripheral>

    <peripheral>
      <name>Chopper</name>
      <description>Chopper Control Registers</description>
      <baseAddress>0x6C</baseAddress>
      <registers>
      </registers>
    </peripheral>

  </peripherals>
</device>
