private:
  template <uint32_t address, uint32_t bankOffset>
  class [dral style="capitalize"]bank.name[#dral]BankReg: public RegisterBankModel<address, bankOffset>
  {
  public:
    [dral template="bank.field.dral"]bank.fields[#dral]
  };

public:
  using [dral style="lowercase"]bank.name[#dral] = [dral style="capitalize"]bank.name[#dral]BankReg<BaseAddress + [dral format="{:#06X}"]bank.offset[#dral], [dral format="{:#06X}"]bank.bank_offset[#dral]>;

